{
  "name": "QMebh",
  "version": "1.0.12",
  "private": true,
  "description": "A Hobby Project Github Bot",
  "author": "Darragh Clarke <darragh.clarke@ucdconnect.ie>",
  "license": "ISC",
  "repository": "https://github.com/DarraghClarke/QMebhBot.git",
  "homepage": "https://github.com/DarraghClarke/QMebhBot",
  "bugs": "https://github.com/DarraghClarke/QMebhBot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^9.5.3",
    "probot-metadata": "git+https://git@github.com/DarraghClarke/metadata.git",
    "probot-scheduler": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
